import java.util.Scanner;

public class ReporteNotas {

    public static void main(String[] args) {
    
    double nota;
    Scanner reporte = new Scanner(System.in);
    
    System.out.println("Introduce la nota que obtuviste");
    nota = reporte.nextDouble();

    if (nota > 5 && nota < 5.99) 
    {
        System.out.println("Aprobaste el curso");
    }
    else if (nota < 5) {
        System.out.println("Has reprobado");
    }
    else if (nota > 6 && nota < 8.99) {
        System.out.println("Eres notable");    
    }   
    else if (nota > 6 && nota < 8.99){
        System.out.println("Eres excelente");

    }
    else
    {
        System.out.println("Introduce nota valida del 1 al 10.");
    }    
    
    }
    
}
